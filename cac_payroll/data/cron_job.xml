<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <!-- Scheduler For Yearly General Ratio, Salary Scale And Employee Job Record Creation -->
        <record id="salary_scale_scheduler" model="ir.cron">
            <field name="interval_type">months</field>
            <field name="name">Salary Scale scheduler</field>
            <field name="numbercall">-1</field>
            <field name="priority">5</field>
            <field name="interval_number">12</field>
            <field name="model_id" ref="model_salary_scale"/>
            <field name="state">code</field>
            <field name="code">model.salary_scale_creation()</field>
        </record>
    </data>
</odoo>
